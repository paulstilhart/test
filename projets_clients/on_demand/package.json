{
  "name": "dt-designsystem",
  "version": "1.0.0",
  "description": "",
  "main": "index.njk",
  "scripts": {
    "serve": "npm-run-all --parallel serve:*",
    "build:tailwind-default": "npx tailwindcss -c tailwind.config.js -i ./app/styles/main.css -o ./app/_includes/assets/css/main-default.css --minify",
    "build:tailwind-lmg": "npx tailwindcss -c tailwind-lmg.config.js -i ./app/styles/main.css -o ./app/_includes/assets/css/main-lmg.css --minify",
    "build": "NODE_ENV=production run-s clean build:*",
    "copy:flowbite": "npx copyfiles --flat ./node_modules/flowbite/dist/flowbite.min.js ./app/_includes/assets/js/",
    "copy:flowbite-datepicker": "npx copyfiles --flat ./node_modules/flowbite-datepicker/dist/js/* ./app/_includes/assets/js/datepicker/",
    "copy": "npm-run-all --parallel copy:*",
    "watch:tailwind-default": "npx tailwindcss -c tailwind.config.js -i ./app/styles/main.css -o ./app/_includes/assets/css/main-default.css --watch",
    "watch:tailwind-lmg": "npx tailwindcss -c tailwind-lmg.config.js -i ./app/styles/main.css -o ./app/_includes/assets/css/main-lmg.css --watch",
    "watch": "npm-run-all --parallel watch:*",
    "dev": "concurrently \"npm run copy\" \"npm run watch\" \"npm run storybook\"",
    "prod": "concurrently \"npm run copy\" \"npm run build\" \"npm run build-storybook\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "@storybook/addon-controls": "^7.6.4",
    "@storybook/addon-docs": "^7.6.4",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-toolbars": "^7.6.5",
    "@storybook/blocks": "^7.4.6",
    "@storybook/html": "^7.4.6",
    "@storybook/html-webpack5": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@whitespace/storybook-addon-html": "^5.1.6",
    "autoprefixer": "^10.4.16",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.4",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.30",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "storybook": "^7.4.6",
    "storybook-addon-themes": "^6.1.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-themer": "^3.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "concurrently": "^8.2.1",
    "date-fns": "^2.30.0",
    "flowbite": "^1.8.1",
    "flowbite-datepicker": "^1.2.4",
    "flowbite-typography": "^1.0.3",
    "lit-html": "^2.8.0",
    "rimraf": "^5.0.5",
    "storybook-theme-switch-addon": "^0.0.6"
  }
}
